{
  "compilerOptions": {
    "target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
    "types": [
      "vite/client",
      "vike-solid/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
    },
  },
  "exclude": [
    "dist"
  ]
}